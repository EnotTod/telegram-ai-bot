{
  "builds": [
    {
      "src": "main.py",
      "use": "@railway/python",
      "config": {
        "runtime": "python3.12",
        "maxInstances": 1,
        "commands": {
          "start": "python main.py"
        }
      }
    }
  ],
  "env": {
    "RUN_MAIN_SCRIPT": "true"
  }
}
